             xmlns:local="clr-namespace:WebCrawler.View.TopicsTree" xmlns:crawling="clr-namespace:WebCrawler.View.Crawling"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
