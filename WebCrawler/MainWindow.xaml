        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WebCrawler" xmlns:uc="clr-namespace:WebCrawler.View.Crawling"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <TabControl>
        <TabItem Header="Crawling">
            <uc:CrawlingTabView Loaded="CrawlingTabView_Loaded"/>
        </TabItem>
        <TabItem Header="Website Tree" >
        </TabItem>
        <TabItem Header="Topics" >
        </TabItem>
    </TabControl>
</Window>
